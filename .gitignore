# We want all custom site settings in settings.local.php
/web/sites/*/settings.local.php
!/web/sites/*/settings.php

# Ignore PhpStorm
.idea

# Ignore private files
private-files/

# Ignore VS-code
.vscode
node_modules/
yarn-error.log

