<html>
<head>
  <meta charset="utf-8">
  <meta name="description" content="{{ title }}">
  <meta name="keywords" content="Selvbetjening">
  <meta name="author" content="{{ author }}">
  <title>{{ title }}</title>
  <link rel="stylesheet" media="all" href="{{ base_url }}/themes/custom/os2forms_selvbetjening_theme/assets/pdf.css" />
</head>
<body>
{% if header %}
  <header>
    {{ header }}
  </header>
{% endif %}

<footer>
  <div class="footer-content">
    {% if footer %}
      {{ footer }}
    {% endif %}
  </div>
  <div class="page-numbers">
    <span>{{ 'Page'|t }}</span>
    <span class="page-number">
      {# Page number inserted by css. #}
    </span>
    <span>{{ 'of'|t }}</span>
    <span class="page-count">
    {# String replace is done by by entity_print_module #}
      {{ '@@PC' }}
    </span>
  </div>
</footer>

<div class="page">
  {{ submission_renderable }}
</div>

{% if debug %}
  {{ dump() }}
{% endif %}

</body>
</html>
