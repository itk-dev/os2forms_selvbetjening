{
    "name": "os2forms/os2forms_rest_api",
    "description": "OS2Forms REST API",
    "type": "drupal-module",
    "license": "MIT",
    "authors": [
        {
            "name": "Mikkel Ricky",
            "email": "rimi@aarhus.dk"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "drupal/key_auth": "^2.0",
        "drupal/webform_rest": "^4.0"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
        "drupal/coder": "^8.3",
        "mglaman/drupal-check": "^1.4"
    },
    "scripts": {
        "code-analysis/drupal-check": [
            "vendor/bin/drupal-check --deprecations --analysis --exclude-dir=vendor *.* src"
        ],
        "code-analysis": [
            "@code-analysis/drupal-check"
        ],
        "coding-standards-check/phpcs": [
            "vendor/bin/phpcs --standard=phpcs.xml.dist"
        ],
        "coding-standards-check": [
            "@coding-standards-check/phpcs"
        ],
        "coding-standards-apply/phpcs": [
            "vendor/bin/phpcbf --standard=phpcs.xml.dist"
        ],
        "coding-standards-apply": [
            "@coding-standards-apply/phpcs"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "enable-patching": true,
        "patches": {
            "drupal/webform_rest": {
                "Allows ability to modify data received the Webform Submission endpoint": "https://raw.githubusercontent.com/itk-dev/os2forms_selvbetjening/feature/DW-585-file-api-endpoint-access/web/modules/custom/os2forms_rest_api/patches/webform_rest_submission.patch"
            }
        }
    }
}
